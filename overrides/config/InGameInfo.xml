            <max>
                <var>light</var>
                <num>7.5</num>
                <str>{yellow}</str>
                <str>{red}</str>
            </max>
            <var>light</var>
            <str> {white}(Feet: </str>
            <max>
                <var>lightnosun</var>
                <num>7.5</num>
                <str>{yellow}</str>
                <str>{red}</str>
            </max>
            <var>lightnosun</var>
            <str>{white})</str>
        </line>
        <line>
            <str>{black}</str>
        </line>
        <line>
            <if>
                <var>snowing</var>
                <str>  It is {darkgreen}{underline}Snowing{reset}</str>
            </if>
            <if>
                <var>raining</var>
                <str>  It is {darkgreen}{underline}Raining{reset}</str>
            </if>
            <if>
                <var>thundering</var>
                <str> and {darkgreen}{underline}Thundering{reset}.</str>
            </if>
        </line>
        <line>
            <str>{black}</str>
        </line>
        <line>
            <if>
                <and>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>-1</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                </and>
                <concat>
                    <icon>
                        <str>textures/items/diamond_pickaxe.png</str>
                        <num>0</num>
                        <num>0</num>
                        <num>16</num>
                        <num>16</num>
                    </icon>
                    <str>  This is an {darkgreen}{underline}ore{white} chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <if>
                <and>
                    <equal>
                        <var>chunkoffsetx</var>
                        <num>7</num>
                    </equal>
                    <equal>
                        <var>chunkoffsetz</var>
                        <num>7</num>
                    </equal>
                </and>
                <concat>
                    <if>
                        <and>
                            <or>
                                <equal>
                                    <modi>
                                        <var>chunkx</var>
                                        <num>3</num>
                                    </modi>
                                    <num>-1</num>
                                </equal>
                                <equal>
                                    <modi>
                                        <var>chunkx</var>
                                        <num>3</num>
                                    </modi>
                                    <num>1</num>
                                </equal>
                            </or>
                            <or>
                                <equal>
                                    <modi>
                                        <var>chunkz</var>
                                        <num>3</num>
                                    </modi>
                                    <num>-1</num>
                                </equal>
                                <equal>
                                    <modi>
                                        <var>chunkz</var>
                                        <num>3</num>
                                    </modi>
                                    <num>1</num>
                                </equal>
                            </or>
                        </and>
                        <str>      </str>
                    </if>
                    <str>  You are at the {darkred}center {white}of the chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <str>{black}</str>
